{
    "manifest_version": 2,
    "name": "Pokédex",
    "version": "1.0",
    "description": "A Pokédex extension",
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "pokeball-logo.svg"
    },
    "permissions": [],
    "icons": {
      "48": "pokeball-logo.svg"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/main.tsx"]
      }
    ]
  }